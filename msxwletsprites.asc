spritestarty equ 254-32
spritecolorsdb:	db 7,11,14,15

spriteinit:
	; first set sprites to zoomed mode {{{
	ld a,(RG1SAV)
	and 252
	or 2
	ld (RG1SAV),a
	ld b,a
	ld c,a
	;call WRTVDP
	ld hl,spritedefs
	ld de,#3800
	ld bc,spritedefsend-spritedefs
	call LDIRVM
	ret
clearspriteatrtabel:
	ld hl,spriteatrtabel
	ld bc,3*256+4
	ld a,128-32
	ex af,af'
	xor a
spriteinit1:
	ld de,256*210+15
	ld (hl),d
	inc hl
	ex af,af'
	ld (hl),a
	inc hl
	add a,32
	ex af,af'
	ld (hl),a
	inc hl
	add a,c
	ld (hl),e
	inc hl
	djnz spriteinit1
	ld b,29
spriteinit2:
	ld a,200 ; do not use 208 since it blocks all other sprites
	ld (hl),a
	inc hl
	ld (hl),a
	inc hl
	xor a
	ld (hl),a
	inc hl
	ld (hl),a
	inc hl
	djnz spriteinit2
transferspriteatrtabel:
	ld de,#1B00
	ld bc,32*4
	ld hl,spriteatrtabel
	call LDIRVM
	ret
	; }}}

spritestartanimation:	; in A is text to scroll
	; {{{
	push af
	ld de,spriteanimation
	call isonroutinestack
	jr nz,spritestartanimation1
	pop af
	ret
spritestartanimation1:
	ld de,spriteanimation
	call addroutine
	pop af
	ld (musicISRvoice),a
	add a,a
	add a,a
	ld c,a
	add a,a
	add a,c
	ld (spriteatrtabel+2),a
	add a,4
	ld (spriteatrtabel+6),a
	add a,4
	ld (spriteatrtabel+10),a
	ld a,spritestarty
	ld (spriteatrtabel),a
	ld (spriteatrtabel+4),a
	ld (spriteatrtabel+8),a
	ld a,40
	ld (spriteatrtabel+1),a
	add a,32
	ld (spriteatrtabel+5),a
	add a,32
	ld (spriteatrtabel+9),a
	xor a
	ld (spritespeedcounter),a
	ret
	; }}}

debugsprites:
	; {{{
	; first check the F-keys before the actual sprite texts
	ld a,(oldnewkey+6)
	ld b,a
	ld a,(NEWKEY+6)
	xor b
	and b
	ld b,a
	bit 5,b
	call nz,debugF1ingame
	bit 6,b
	call nz,debugF2ingame
	bit 7,b
	call nz,debugF3ingame
	ld a,(oldnewkey+7)
	ld b,a
	ld a,(NEWKEY+7)
	xor b
	and b
	ld b,a
	bit 0,b
	call nz,debugF4ingame
	bit 1,b
	call nz,debugF5ingame

debugsprites1

debugsprites8
	ld a,(NEWKEY+6)
	ld (oldnewkey+6),a
	ld a,(NEWKEY+7)
	ld (oldnewkey+7),a
	ld de,spriteanimation
	call isonroutinestack
	ret z
	ld a,(NEWKEY+0)
	xor 255
	ret z
	ld e,a
	xor a
	ld b,7
debugsprites19
	rr e
	jp c,spritestartanimation
	inc a
	djnz debugsprites19
	ret
debugF1ingame:
debugF2ingame:
debugF3ingame:
debugF4ingame:
	ret
debugF5ingame:
	ld a,1
	ld (gameover),a
	ret
	; }}}

spriteanimation:
	; {{{
	ld a,(spritespeedcounter)
	or a
	jr nz,spriteanimation1
	inc a
	inc a
	inc a
	jr spriteanimation5
spriteanimation1
	cp 10
	jr z,spriteanimation4
	cp 15
	jr z,spriteanimation4
	cp 20
	jr z,spriteanimation4
	cp 30
	jr z,spriteanimation4
	cp 35
	jr z,spriteanimation4
	cp 40
	jr z,spriteanimation4
	cp 45
	jr z,spriteanimation4

	ld a,(spritespeed)
	jr spriteanimation6
spriteanimation4:
	ld a,(spritespeed)
	dec a
	jr nz,spriteanimation5
	;start sfx nu we are at bottom of curve
	ld a,(musicISRvoice)
	add a,a
	ld d,0
	ld e,a
	ld hl,voicespointer
	add hl,de
	ld e,(hl)
	inc hl
	ld d,(hl)
	ld (PNT_FRAME),de
	ld a,(musicISRvoice)
	or 128
	ld e,a
	ld hl,0
	call startsfx
	xor a
spriteanimation5:
	ld (spritespeed),a
spriteanimation6:
	ld c,a
	ld a,(spriteatrtabel)
	add a,c
	ld (spriteatrtabel),a
	ld (spriteatrtabel+4),a
	ld (spriteatrtabel+8),a
	cp 192
	jr c,spriteanimation7
	cp spritestarty
	jr nc,spriteanimation7
	ld de,spriteanimation
	call delroutine
spriteanimation7:

	ld a,(spritespeedcounter)
	inc a
	ld (spritespeedcounter),a
	and 3
	ld hl,spritecolorsdb
	add a,l
	ld l,a
	ld a,(hl)
	ld (spriteatrtabel+3),a
	ld (spriteatrtabel+7),a
	ld (spriteatrtabel+11),a
	ld de,#1B00
	ld bc,32*4
	ld hl,spriteatrtabel
	call LDIRVM
	ret
	; }}}


