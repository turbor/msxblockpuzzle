startsfx1:
	ld hl,musicpiecesfx1
	jr startsfx
startsfx2:
	ld hl,musicpiecesfx2
	jr startsfx
startsfx3:
	ld hl,musicpiecesfx3
startsfx:
	;not when ai is moving the pieces!!
	ld a,(ai_active)
	or a
	ret nz
	ld (musicpointer),hl
	ld a,1
	ld (musicwaitint),a
	ret

startmusic:
	ld a,1
startmusic2:
	ld (musicwaitint),a
	ld hl,musicpiece
	ld (musicpointer),hl
	ret
stopmusic:
	ld hl,musicpieceend
	ld (musicpointer),hl
	jr musicplayer2

musicplayer:
	ld a,(musicwaitint)
	dec a
	ld (musicwaitint),a
	jp nz,oldhook
musicplayer2:	ld a,10
	ld (musicwaitint),a
	ld hl,(musicpointer)
	ld a,(hl)
	or a
	jp z,oldhook ; end of music reached so do nothing

	inc hl
	ld b,a
	ld c,#a1
	and 1
	jr z,musicplayerchanB
	bit 1,b
	jr z,musicplayerchanAnote
	; volume A
	ld a,8
	out (#a0),a
	nop
	outi
	inc b
	; note A
musicplayerchanAnote:	bit 2,b
	jr z,musicplayerchanB
	xor a
	ld d,a
	out (#a0),a
	push hl
	ld a,(hl)
	add a,a
	ld e,a
	ld hl,musicnotes
	add hl,de
	outi
	ld a,1
	out (#a0),a
	nop
	outi
	pop hl
	inc hl
	inc b
	inc b

musicplayerchanB:
	bit 4,b
	jr z,musicplayerchanend
	bit 5,b
	jr z,musicplayerchanBnote
	; volume B
	ld a,9
	out (#a0),a
	nop
	outi
	inc b
	; note B
musicplayerchanBnote:	bit 6,b
	jr z,musicplayerchanend
	xor a
	ld d,a
	ld a,2
	out (#a0),a
	push hl
	ld a,(hl)
	add a,a
	ld e,a
	ld hl,musicnotes
	add hl,de
	outi
	ld a,3
	out (#a0),a
	nop
	outi
	pop hl
	inc hl

musicplayerchanend:
	ld (musicpointer),hl
	; now continue with old hook
	jp oldhook


