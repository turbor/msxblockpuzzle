insertscoreinscroll:	; in hl,de
	ld b,5
insertscoreinscroll0:; first skip leading zero's
	ld a,(hl)
	or a
	jr nz,insertscoreinscroll1
	inc hl
	djnz insertscoreinscroll0
insertscoreinscroll1:
	ld a,(hl) ; last inc hl before djnz fall through....
	cp 255
	ret z

	ld (de),a
	inc de
	inc hl
	jr insertscoreinscroll1

scrollbigtextinsertscore:
	inc hl
	push hl
	ld hl,realscore+1
	call insertscoreinscroll
	pop hl
	jr scrollbigtext0
scrollbigtextinserthiscore:
	inc hl
	push hl
	ld hl,hiscore+1
	call insertscoreinscroll
	pop hl
	jr scrollbigtext0


scrollbigtext:
	; in HL text to scroll {{{
	ld de,scrolltext
scrollbigtext0:
	ld a,(hl)
	or a
	jr z,scrollbigtext3
	cp 32
	jr z,scrollbigtextspaceadded
	cp '>'
	jr z,scrollbigtextinserthiscore
	cp '<'
	jr z,scrollbigtextinsertscore
	sub '0'
	cp 10
	jr c,scrollbigtext1
	sub 7
	cp 36
	jr c,scrollbigtext1
	ld a,36
scrollbigtext1:
	ld (de),a
	inc hl
	inc de
	jr scrollbigtext0
scrollbigtextspaceadded:
	ld a,36
	jr scrollbigtext1

scrollbigtext3: ; start actual scroll here
	ld a,255
	ld (de),a ; final closing marker
	ld hl,nametabel+31+6*32
	ld (DX),hl
	ld a,32
	ld (DWidth),a
	ld a,8
	ld (SWidth),a
	ld (NY),a
	ld a,1
	ld (NX),a

	ld a,(scrolltextcount)
	dec a
	ld (scrolltextcount),a


scrollbigtext4:
	ld de,scrolltext
	ld (scrolltextpnt),de
	ld a,(scrolltextcount)
	inc a
	ld (scrolltextcount),a
	ld e,a
	ld a,(scrolltextmaxcount)
	cp e
	ret z

scrollbigtext5:
	ld de,(scrolltextpnt)
	ld a,(de)
	cp 255
	jr z,scrollbigtext4
	inc de
	ld (scrolltextpnt),de
	add a,a ; *2
	add a,a ; *4
	ld l,a
	ld h,0
	add hl,hl ; *8
	ld de,bigscrolchars
	add hl,de
	call chartobig
	ld hl,workspace+100
	ld (SX),hl
	ld b,6
	xor a
	ld (waitinterrupted),a
scrollbigtext6:
	push bc
	ld de,nametabel+6*32
	ld hl,nametabel+1+6*32
	ld bc,32*7
	ldir
	call doboxcopy
	ld hl,nametabel
	ld de,#1800
	ld bc,32*24
	call LDIRVM

	ld hl,(SX)
	inc hl
	ld (SX),hl
	call bigscrollerwait
	call bigscrollerwait
	call bigscrollerwait
	pop bc
	djnz scrollbigtext6

	ld a,(waitinterrupted)
	or a

	jp z,scrollbigtext5

scrollbigtext8:
	xor a
	ld (waitinterrupted),a
	call bigscrollerwait
	ld a,(waitinterrupted)
	or a
	jr nz,scrollbigtext8
	ret
	;}}}

bigscrollerwait:
	ei
	halt
	call getjoystick
	and #30
	xor #30
	ld e,a
	ld a,(waitinterrupted)
	or e
	ld (waitinterrupted),a
	ret


chartobig:	
	; in hl points to char {{{
	push hl
	; clear 10x10 workspace (needed for 8x8 char and 1 bit border on all sides
	ld hl,workspace
	ld de,workspace+1
	ld bc,100
	xor a
	ld (hl),a
	ldir

	ld hl,workspace+8+1
	pop de
	ld bc,#08ff
chartobig2:
	push bc
	ld a,(de)
	inc de
	ld b,8
chartobig4:	add a,a
	jr nc,chartobig6
	ld (hl),c
chartobig6:	inc hl
	djnz chartobig4
	inc hl
	inc hl
	pop bc
	djnz chartobig2

	; now copy this 10x10 to workspace+100 but width the correct chars 224-255...
	ld ix,workspace+1+8
	ld hl,workspace+100
	ld b,8
chartobig8:
	push bc
	ld b,8
chartobig10:
	ld e,7
	ld a,(ix+0)
	add a,a
	rl e
	ld a,(ix+1)
	add a,a
	rl e
	ld a,(ix-1)
	add a,a
	rl e
	ld a,(ix+10)
	add a,a
	rl e
	ld a,(ix-10)
	add a,a
	rl e
	inc ix
	ld (hl),e
	inc hl
	djnz chartobig10
	inc ix
	inc ix
	pop bc
	djnz chartobig8
	ret ;}}}

createbigcharset:
	; this charset goes from 224 up to 255 {{{
	; %11100000 to %11111111
	; the lower bits have the follwoing meaning
	; SelfFilled,Right,Left,Down,Up ; 1 means filled
	xor a
	ld hl,workspace
	ld de,workspace+1
	ld (hl),a
	ld bc,32*8
	ldir
	ld de,workspace+8*5
	ld hl,bigchartopleft
	ld bc,4
	ldir
	ld de,workspace+8*6+4
	ld hl,bigcharbottomleft
	ld bc,4
	ldir
	ld de,workspace+8*7
	ld hl,bigchartopleft
	ld bc,8
	ldir

	ld de,workspace+8*9
	ld hl,bigchartopright
	ld bc,4
	ldir
	ld de,workspace+8*10+4
	ld hl,bigcharbottomright
	ld bc,4
	ldir
	ld de,workspace+8*11
	ld hl,bigchartopright
	ld bc,8
	ldir

	ld b,8
	ld ix,workspace+8*7
	ld de,workspace+8*11
	ld hl,workspace+8*15
createbigcharset1:
	ld a,(de)
	or (ix+0)
	ld (hl),a
	inc ix
	inc hl
	inc de
	djnz createbigcharset1

	ld de,workspace+8*13
	ld hl,workspace+8*15
	ld bc,4
	ldir
	ld de,workspace+8*14+4
	ld bc,4
	ldir


	; now for the upper 16 chars
	ld b,16
	ld hl,workspace+8*15
	ld de,workspace+8*16
createbigcharset2:
	push bc
	ld b,8
createbigcharset3:
	ld a,255
	xor (hl)
	ld (de),a
	inc hl
	inc de
	djnz createbigcharset3
	ld bc,-16
	add hl,bc
	pop bc
	djnz createbigcharset2

	; now to vram
	ld hl,workspace
	ld de,224*8
	ld bc,32*8
	call LDIRVM	; copy patern table

	ld hl,workspace
	ld de,224*8+256*8
	ld bc,32*8
	call LDIRVM	; copy patern table

	ld hl,workspace
	ld de,224*8+512*8
	ld bc,32*8
	call LDIRVM	; copy patern table
	;now colors
	ld de,workspace+1
	ld hl,workspace
	ld bc,32*8
	ld a,#12
	ld (hl),a
	ldir

	ld hl,workspace
	ld de,224*8+0x2000
	ld bc,32*8
	call LDIRVM	; copy color table
	ld hl,workspace
	ld de,224*8+256*8+0x2000
	ld bc,32*8
	call LDIRVM	; copy color table
	ld hl,workspace
	ld de,224*8+512*8+0x2000
	ld bc,32*8
	call LDIRVM	; copy color table
	ret	; }}}


bigchartopleft: ; {{{
	db %11110000
	db %11000000
	db %10000000
	db %10000000
	;}}}
bigcharbottomleft: ; {{{
	db %10000000
	db %10000000
	db %11000000
	db %11110000
	;}}}
bigchartopright: ; {{{
	db %00001111
	db %00000011
	db %00000001
	db %00000001
	;}}}
bigcharbottomright: ; {{{
	db %00000001
	db %00000001
	db %00000011
	db %00001111
	;}}}


